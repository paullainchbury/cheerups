  <tr class="cheerup">
    <td><%= cheerup.title %></td>
    <td><%= cheerup.sub_title %></td>
    <td><%= cheerup.cheerpoints %></td>
    <td><%= link_to 'UpVote', vote_cheerup_path(cheerup, value: 1), method: "post" %> | <%= link_to 'DownVote', vote_cheerup_path(cheerup, value: -1), method: "post" %></td>
    <td><%= cheerup.user.email %>(<%=cheerup.user.cheerpoints %>)</td>
    <td><%= "#{time_ago_in_words(cheerup.created_at)} ago "%></td>
    <td><%= link_to 'Show', cheerup %></td>
    <td><%= link_to 'Edit', edit_cheerup_path(cheerup) %></td>
    <td><%= link_to 'Destroy', cheerup, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>

